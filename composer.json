            "role": "Developer"
        }
    ],
    "support": {
        "issues": "https://github.com/mfn/php-reflection-gen/issues"
    },
    "license": "MIT",
    "require": {
        "php": ">=5.4.0",
        "symfony/console": "~2.5"
